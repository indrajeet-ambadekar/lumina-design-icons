{
  "name": "lumina-design-icons",
  "version": "1.0.21",
  "description": "Made with create-react-library",
  "author": "indrajeet-ambadekar",
  "license": "MIT",
  "repository": "https://github.com/indrajeet-ambadekar/lumina-design-icons.git",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "npm run build-icons && microbundle-crl --no-compress --format modern,cjs --globals react-date-picker",
    "start": "npm run build-icons && microbundle-crl watch --no-compress --format modern,cjs",
    "watch": "onchange 'src/**/*.**' 'example/**/*.**' './package.json' 'README.md' -- prettier --write {{changed}}",
    "prepare": "run-s build",
    "prebuild": "rimraf icon-src && rimraf icon-dist",
    "build-icons": "npm run prebuild && npm run svgr && rollup -c && npm run copy-icons-build",
    "copy-icons-build": "rimraf src/components/icons/icon-lib.js && mv icon-dist/icon-lib.js src/components/icons/icon-lib.js",
    "svgr": "svgr --icon --title-prop --replace-attr-values '#292929bf=currentColor' -d icon-src -- icons-svg",
    "predeploy": "cd example && npm install && npm run build",
    "add_package_json": "cp -a ./config/* ./dist/ && cp -a README.md dist/.",
    "deploy": "gh-pages -d example/build",
    "publish_package": "npm run build && npm run add_package_json && cd dist && npm publish && cd ../",
    "custom-svgr": "rm -rf custom-icon-src/* && svgr --icon --title-prop --replace-attr-values '#292929bf=currentColor' -d custom-icon-src -- custom-icon-svg"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-date-picker": "^8.0.7",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "react-transition-group": "^4.4.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-unicode-property-regex": "^7.18.6",
    "@rollup/plugin-babel": "^6.0.3",
    "@svgr/cli": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "prettier": "^2.8.7",
    "react": "^17.0.2",
    "react-date-picker": "^8.0.7",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "react-transition-group": "^4.4.1",
    "rollup": "^1.32.1",
    "rollup-plugin-filesize": "^10.0.0",
    "sass": "^1.62.0"
  },
  "files": [
    "dist"
  ]
}
